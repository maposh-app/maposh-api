{
  "name": "serverless-graphql",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql.git"
  },
  "scripts": {
    "pretest": "./utils/scripts/pre.sh",
    "test": "jest",
    "dynamodb-generate-seed-data": "cd utils/seed-data && node create_seed_data.js",
    "dynamodb-insert-seed-data-prod": "cd lib/seed-data && node insert_seed_data_prod.js",
    "prestart": "./utils/scripts/pre.sh",
    "start": "yarn dynamodb-generate-seed-data && serverless dynamodb install --localPath ./bin && env-cmd .env.local serverless offline start",
    "generate-types": "yarn gql-gen -s tmp/schema.json -t graphql-codegen-typescript-resolvers-template --out ./types/resolvers/index.d.ts",
    "deploy-prod": "yarn dynamodb-generate-seed-data && env-cmd .env.prod serverless --stage=production deploy && yarn dynamodb-insert-seed-data-prod"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-server-lambda": "^2.4.8",
    "aws-xray-sdk": "^2.2.0",
    "colors": "^1.3.3",
    "graphql": "^14.1.1",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.4",
    "merge-graphql-schemas": "^1.5.8",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.19",
    "@types/colors": "^1.2.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.9.5",
    "@types/uuid": "^3.4.4",
    "apollo": "^2.5.3",
    "aws-sdk": "^2.411.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.15.2",
    "cors": "2.8.5",
    "env-cmd": "^8.0.2",
    "faker": "^4.1.0",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-typescript-resolvers-template": "^0.17.0",
    "graphql-import-loader": "^0.2.1",
    "graphql-tag": "^2.10.1",
    "imports-loader": "^0.8.0",
    "jest": "^24.1.0",
    "jsonfile": "^5.0.0",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "0.2.36",
    "serverless-offline": "^4.7.0",
    "serverless-plugin-typescript": "^1.1.5",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-node-externals": "^1.6.0"
  }
}
