{
  "name": "maposh-ai",
  "version": "0.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/maposh-app/maposh-api"
  },
  "scripts": {
    "pretest": "./src/utils/scripts/pre.sh",
    "test": "jest",
    "dynamodb-generate-seed-data": "cd ./src/utils/seed-data && node create_seed_data.js",
    "dynamodb-insert-seed-data-staging": "cd ./src/utils/seed-data && node insert_seed_data_staging.js",
    "prestart": "./src/utils/scripts/pre.sh",
    "start": "yarn dynamodb-generate-seed-data && serverless dynamodb install --localPath ./bin && env-cmd .env serverless offline start",
    "deploy-staging": "yarn dynamodb-generate-seed-data && env-cmd .env serverless --stage=staging deploy && yarn dynamodb-insert-seed-data-staging"
  },
