{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "no-console": {
      "severity": "warning"
    },
    "object-literal-sort-keys": false,
    "interface-name": false,
    "variable-name": [false],
    "no-submodule-imports": false,
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}
